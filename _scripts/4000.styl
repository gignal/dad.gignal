@font-face {
  font-family: '4000heavy_left';
  src: url('font/4000-heavy_left-webfont.eot');
  src: url('font/4000-heavy_left-webfont.eot?#iefix') format('embedded-opentype'),
       url('font/4000-heavy_left-webfont.woff') format('woff'),
       url('font/4000-heavy_left-webfont.ttf') format('truetype'),
       url('font/4000-heavy_left-webfont.svg#4000heavy_left') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: '4000italic';
  src: url('font/4000-italic-webfont.eot');
  src: url('font/4000-italic-webfont.eot?#iefix') format('embedded-opentype'),
       url('font/4000-italic-webfont.woff') format('woff'),
       url('font/4000-italic-webfont.ttf') format('truetype'),
       url('font/4000-italic-webfont.svg#4000italic') format('svg');
  font-weight: normal;
  font-style: normal;
}
