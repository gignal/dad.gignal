<div class="gignal-unibox">
  {{#photo}}
    <div class="gignal-image" style="background-image:url('{{photo}}');"></div>
  {{/photo}}
  {{#message}}
    <p class="message">{{{message}}}</p>
  {{/message}}
</div>

{{> footer}}

<div class="gignal-toolbox">
	<span class="gignal-service">
		<a href="{{link}}" target="_blank">
			<p>{{service}}</p>
		</a>
	</span>
	<span class="gignal-tool">

    <span class="st_facebook_custom" displayText="" st_title="{{message}}" st_url="{{link}}" st_image="{{photo}}">
      <img src="./gignal/images/Facebook-white.png">
    </span>
    <span class="st_twitter_custom" displayText="" st_title="{{message}}" st_url="{{link}}" st_image="{{photo}}" st_via="gignal">
      <img src="./gignal/images/Twitter-white.png">
    </span>
    <span class="st_instagram_custom" displayText="" st_title="{{message}}" st_url="{{link}}" st_image="{{photo}}">
      <img src="./gignal/images/Instagram-white.png">
    </span>

	</span>
</div>